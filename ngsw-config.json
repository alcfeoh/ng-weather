{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
      "name": "WeatherAPI",
      "urls": [
        "http://api.openweathermap.org/data/**"
      ],
      "cacheConfig": {
        "maxAge": "6h",
        "strategy": "freshness"
      }
    }]
}